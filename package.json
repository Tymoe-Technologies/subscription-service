{
  "name": "subscription-service",
  "version": "1.0.0",
  "description": "Authorization + billing hub for tymoe products",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "verify:logic": "tsx scripts/verify-logic.ts",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/scripts/seed-data.ts",
    "prisma:seed-entitlements": "tsx src/scripts/seed-entitlements.ts",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "subscription",
    "billing",
    "stripe",
    "redis",
    "prisma"
  ],
  "author": "Tymoe Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.3.2",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "nanoid": "^5.0.6",
    "pg": "^8.11.3",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "stripe": "^14.20.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
